<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="car" class="com.david.lesson5.Car" autowire="byName">
	<property name="name" value="mercedes"></property>
	</bean>
	<bean id="wheel" class="com.david.lesson5.Wheel">
	<property name="radius" value="10"></property>
	<property name="rimType" value="type 1"></property>
	</bean>

</beans>
