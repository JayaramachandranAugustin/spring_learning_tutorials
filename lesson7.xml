<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="bike" class="com.david.lesson7.Bike">
	<property name="engineCC" value="180CC"></property>
	<property name="mileage" value="75"></property>
	</bean>
	<bean id="ducati" class="com.david.lesson7.Ducati" autowire="constructor">
	<constructor-arg type="java.lang.String" value="4 stroke"></constructor-arg>
	<constructor-arg type="int" value="250000"></constructor-arg>
	<constructor-arg type="int"  value="2014"></constructor-arg>
	
	</bean>
</beans>
