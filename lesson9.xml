<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.2.xsd">

<context:annotation-config></context:annotation-config>

	<bean id="pencil" class="com.david.lesson9.Pencil">
	<property name="company" value="Camelin"></property>
	</bean>
	<bean id="pencil1" class="com.david.lesson9.Pencil">
	<property name="company" value="Natraj"></property>
	</bean>
	<bean id="colorpencil" class="com.david.lesson9.ColorPencil"></bean>
</beans>
